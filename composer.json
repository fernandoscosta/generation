{
    "name": "nandosc/generation",
    "description": "Stp generator for PHP.",
    "license": "MIT",
    "homepage": "http://github.com/fernandoscosta/nandosc",
    "authors": [
        {
            "name": "Fernando Costa",
            "email": "fernandocostasud@gmail.com",
            "homepage": "https://github.com/fernandoscosta/nandosc",
            "role": "Developer"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "http://github.com/fernandoscosta/nandosc"
        }
    ],
    "require": {
        "php": "^8.2",
        "ext-iconv": "*"
    },
    "autoload": {
        "psr-4": {
            "Nandosc\\": "src/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^10.4",
        "pestphp/pest": "^2.0"
    },
    "config": {
        "allow-plugins": {
            "ocramius/package-versions": true,
            "pestphp/pest-plugin": true
        }
    },
    "archive": {
        "exclude": [
            "/test",
            "/phpunit.xml.dist"
        ]
    }
}